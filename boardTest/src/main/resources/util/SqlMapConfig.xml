<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"  
 "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>


	<settings>            <!-- <settings>이 없으면 만들어줍니다. -->

		<setting name="jdbcTypeForNull" value="NULL" />

	</settings>


	<typeAliases>
		<typeAlias alias="BoardList"
			type="com.test.boar.model.BoardList" />
		<typeAlias alias="BoardUpdate"
			type="com.test.boar.model.BoardUpdate" />
		<typeAlias alias="BoardWrite"
			type="com.test.boar.model.BoardWrite" />
		<typeAlias alias="BoardTest"
			type="com.test.boar.model.BoardTest" />

	</typeAliases>

	<mappers>
		<mapper resource="sql/member.xml"></mapper>
		<mapper resource="sql/board.xml"></mapper>

	</mappers>
</configuration>